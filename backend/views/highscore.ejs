<section class="highscore-content">

    <h1 class="highscore-title"> Highscore List </h1>

    <div class="highscore-list">

        <% list.forEach(function(item) { %>
        <div class="highscore-item">
                <p>Name: <%= item.firstName%></p>
                <p>Time: <%= item.completionTime%></p>
                <p><%= item.wordLength%>-letter <%= item.uniqueLetters%></p>
                <p>Number of guesses: <%= item.numberOfGuesses%></p>
        </div>
        <% }); %>  

        
        
    </div>
</section>

